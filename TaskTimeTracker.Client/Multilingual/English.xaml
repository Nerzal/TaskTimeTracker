<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--  MainWindow  -->
    <system:String x:Key="MainWindow.Title">Timestamps</system:String>
    <system:String x:Key="MainWindow.AddButton.Content">Add</system:String>
    <system:String x:Key="MainWindow.ConfigButton.Content">Config</system:String>

    <!--  Tasklist Entry  -->
    <system:String x:Key="TaskList.Entry.EditButton.Content">Edit</system:String>
    <system:String x:Key="TaskList.Entry.SaveButton.Content">Save</system:String>
    <system:String x:Key="TaskList.Entry.AbortButton.Content">Abort</system:String>
    <!--  ContextMenu  -->
    <system:String x:Key="TaskList.Entry.ContextMenu.Copy">Copy</system:String>
    <system:String x:Key="TaskList.Entry.ContextMenu.Delete">Delete</system:String>

    <!--  Inbox Window  -->
    <system:String x:Key="Inbox.Title">Add new Timestamp</system:String>
    <system:String x:Key="Inbox.Buttons.Ok.Content">Ok</system:String>
    <system:String x:Key="Inbox.Buttons.Abort.Content">Abort</system:String>
    <system:String x:Key="Inbox.TextBlocks.Description.Text">Description:</system:String>

