    <system:String x:Key="ConfigurationWindow.TextBlocks.StampText.Text">Text for Stamp</system:String>
    <system:String x:Key="ConfigurationWindow.CheckBoxes.Ctrl.Content">Ctrl</system:String>
    <system:String x:Key="ConfigurationWindow.CheckBoxes.Windows.Content">Windows</system:String>
    <system:String x:Key="ConfigurationWindow.CheckBoxes.Alt.Content">Alt</system:String>
    <system:String x:Key="ConfigurationWindow.CheckBoxes.StampOnStartup.Content">Set Stamp on Startup</system:String>
    <system:String x:Key="ConfigurationWindow.Buttons.Ok.Content">Ok</system:String>
    <system:String x:Key="ConfigurationWindow.Buttons.Cancel.Content">Cancel</system:String>
    <system:String x:Key="ConfigurationWindow.CheckBoxes.StampOnLockScreen.Content">Set Stamp on Lockscreen</system:String>
    <system:String x:Key="ConfigurationWindow.TextBlocks.ScreenLocked.Text">Stamp when locked</system:String>
    <system:String x:Key="ConfigurationWindow.TextBlocks.ScreenUnlocked.Text">Stamp when unlocked</system:String>
